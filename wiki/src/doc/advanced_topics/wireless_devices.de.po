#| "       0: phy0: Wireless LAN\n"
#| "               Soft blocked: no\n"
#| "               Hard blocked: no\n"
#| "       1: hci0: Bluetooth\n"
#| "               Soft blocked: no\n"
#| "               Hard blocked: no\n"
#| "       2: gps0: GPS\n"
#| "               Soft blocked: no\n"
#| "               Hard blocked: no\n"
msgid ""
"       0: phy0: Wireless LAN\n"
